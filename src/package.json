{
  "name": "merlin-web-audio",
  "version": "0.0.1",
  "description": "Working with audio on the web",
  "dependencies": {
    "@tensorflow/tfjs": "^2.0.1",
    "@types/glob": "^7.1.3",
    "@types/terser-webpack-plugin": "^4.1.0",
    "audio-recorder-polyfill": "^0.3.6",
    "fft.js": "^4.0.3",
    "ndarray": "^1.0.19",
    "ndarray-resample": "^1.0.1",
    "@types/webgl2": "0.0.5"
  },
  "devDependencies": {
    "@types/dom-mediacapture-record": "^1.0.7",
    "@types/ndarray": "^1.0.6",
    "http-server": "^0.12.3",
    "terser-webpack-plugin": "^1.4.5",
    "ts-loader": "^5.3.0",
    "ts-node": "^5.0.1",
    "tsify": "^3.0.4",
    "tslint": "^5.9.1",
    "typescript": "^3.6.3",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9"
  },
  "scripts": {
    "build": "npm run clean && webpack --config webpack/es6.config.ts",
    "lint": "tslint -c tslint.json -p ./tsconfig.es6.json -t verbose",
    "clean": "rm -rf ./es6",
    "clean-bundles": "find demos/ -name \"*_bundle.js\" -print -delete",
    "sslgen-demos": "chmod +x keys/sslgen.sh && ./keys/sslgen.sh",
    "build-demos": "webpack --config webpack/es6.demo.config.ts",
    "serve-demos": "http-server -S -K keys/demos.key -C keys/demos.crt demos/"
  },
  "author": "Grant Van Horn",
  "license": "Apache-2.0"
}
