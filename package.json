{
  "name": "tmpui-template",
  "version": "0.3.0",
  "private": true,
  "proxy": "http://localhost:5000",
  "scripts": {
    "devel": "react-scripts build && npm run-script start-flask",
    "start-react": "react-scripts start",
    "start-flask": "cd apipy && venv/bin/flask run --no-debugger",
    "start-next": "next start -p $PORT",
    "build": "react-scripts build",
    "heroku-postbuild": "npm run build"
  },
  "dependencies": {
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@material-ui/core": "^4.11.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.3",
    "@svgr/webpack": "^5.4.0",
    "@types/node": "^14.6.0",
    "@types/terser-webpack-plugin": "^4.1.0",
    "fft.js": "^4.0.3",
    "glob": "^7.1.6",
    "minimist": "^1.2.5",
    "ndarray": "^1.0.19",
    "ndarray-resample": "^1.0.1"
  },
  "devDependencies": {
    "@material-ui/core": "^4.11.0",
    "@types/ndarray": "^1.0.6"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "eslintConfig": {
    "extends": "tmpui-template"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
