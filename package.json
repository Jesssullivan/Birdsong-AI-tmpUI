{
  "name": "tmp-record-annotate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "sslgen-demos": "chmod +x keys/sslgen.sh && ./keys/sslgen.sh",
    "build-demos": "webpack --config webpack/bundle.config.ts",
    "serve-demos": "http-server -S -K keys/demos.key -C keys/demos.crt demos/",
    "clean-bundles": "find demos/ -name \"*_bundle.js\" -print -delete",
    "clean-all": "npm run-script clean-bundles && rm package-lock.json && rm -rf node_modules",
    "dev": "next dev",
    "build": "next build",
    "run": "next build && next start",
    "start": "next start -p $PORT",
    "heroku-postbuild": "npm run build"
  },
  "dependencies": {
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@svgr/webpack": "^5.4.0",
    "@tensorflow/tfjs": "^2.0.1",
    "@types/glob": "^7.1.3",
    "@types/node": "^14.6.0",
    "@types/terser-webpack-plugin": "^4.1.0",
    "audio-recorder-polyfill": "^0.3.6",
    "domino": "^2.1.6",
    "fft.js": "^4.0.3",
    "ndarray": "^1.0.19",
    "ndarray-resample": "^1.0.1",
    "next": "9.3.5",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-easy-crop": "^3.1.1",
    "react-mic": "^12.4.6"
  },
  "devDependencies": {
    "@babel/core": "7.11.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@material-ui/core": "^4.11.0",
    "@types/dom-mediacapture-record": "^1.0.7",
    "@types/ndarray": "^1.0.6",
    "http-server": "^0.12.3",
    "terser-webpack-plugin": "^1.4.5",
    "ts-loader": "^5.3.0",
    "ts-node": "^5.0.1",
    "tsify": "^3.0.4",
    "tslint": "^5.9.1",
    "typescript": "^3.6.3",
    "webpack": "4.28.4",
    "webpack-cli": "^3.3.9"
  }
}
